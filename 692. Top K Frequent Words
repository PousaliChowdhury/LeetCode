class Solution:
    def topKFrequent(self, words: List[str], k: int) -> List[str]:
        freq = {}
        for word in words:
            freq[word] = freq.get(word, 0)+1

        sorted_items = sorted(freq.items(), key = lambda x: (-x[1], x[0]))

        result = []
        for i in range(k):
            result.append(sorted_items[i][0])
        return result
