class Solution(object):
    def rotate(self, nums, k):
        n = len(nums)
        if k>n: k = k%n
        def reverse(l,r):
            while l<r:
                nums[r], nums[l] = nums[l], nums[r]
                l += 1
                r -= 1
        reverse(0, n-1)
        reverse(0, k-1)
        reverse(k, n-1)
        
