class Solution:
    def subarraysDivByK(self, nums: List[int], k: int) -> int:
        count = 0
        total_sum = 0
        freq = {0:1}   
        for i in nums:
            total_sum += i
            rem = total_sum % k
            if rem in freq:
                count += freq[rem]
            freq[rem] = freq.get(rem, 0)+1
        return count
