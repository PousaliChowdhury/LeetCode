class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        nums_set = set(nums)
        Lcount = 0
        nums.sort()
        for i in nums_set:
            if i-1 not in nums_set:
                current = i
                length = 1
                while current+1 in nums_set:
                    current += 1
                    length += 1
                Lcount = max(Lcount, length)
        return Lcount
            
