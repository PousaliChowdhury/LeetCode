def chocolateFactory(arr):
    j = 0
    for i in range(len(arr)):
        if arr[i] != 0:
            arr[j] = arr[i]
            j += 1
        
    while j < len(arr):
        arr[j] = 0
        j += 1
    return arr
    
if __name__ == "__main__":
    n = int(input())
    arr = list(map(int, input().split()))
    result = chocolateFactory(arr)
    print(result)
            
