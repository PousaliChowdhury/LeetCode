class Solution:
    def sumFourDivisors(self, nums: List[int]) -> int:
        ans = 0
        for x in nums:
            divs = set()
            for i in range(1, int(math.sqrt(x)) + 1):
                if x % i == 0:
                    divs.add(i)
                    divs.add(x // i)
                if len(divs) > 4:
                    break
            if len(divs) == 4:
                ans += sum(divs)

        return ans
