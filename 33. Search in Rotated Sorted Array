//Python
class Solution:
    def search(self, nums: List[int], target: int) -> int:
        n = len(nums)
        for i in range(n):
            if nums[i] == target:
                return i
        return -1


//Java
class Solution {
    public int search(int[] nums, int target) {
        int t = nums.length;
        for(int i=0; i<t; i++){
            if(nums[i] == target)
                return i;
        }
        return -1;
    }
}
